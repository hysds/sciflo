<?xml version="1.0"?>
<scifloConfig xmlns="http://sciflo.jpl.nasa.gov/2006v1/sf"
        xmlns:sf="http://sciflo.jpl.nasa.gov/2006v1/sf"
        xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <workUnitRootWorkDir>/tmp/workUnitRootWorkDir2</workUnitRootWorkDir>
    <workUnitStoreType>bsddb</workUnitStoreType>
    <workUnitStoreHome>/tmp/workUnitStoreHome2</workUnitStoreHome>
    <workUnitStoreName>workUnits</workUnitStoreName>
    <workUnitStoreDb>workUnits.db</workUnitStoreDb>
    <scheduleStoreType>bsddb</scheduleStoreType>
    <scheduleStoreHome>/tmp/scheduleStoreHome2</scheduleStoreHome>
    <scheduleStoreName>schedule</scheduleStoreName>
    <scheduleStoreDb>schedule.db</scheduleStoreDb>
    <gridProtocol>ssl</gridProtocol>
    <gridPort>9853</gridPort>
    <gridNamespace>http://sciflo.jpl.nasa.gov/2006v1/sf/GridService</gridNamespace>
    <addWorkUnitMethod>
        <exposedName>addAndExecuteWorkUnit</exposedName>
        <pythonFunction>testGridModule.addAndExecuteWorkUnitTest2</pythonFunction>
    </addWorkUnitMethod>
    <queryWorkUnitMethod>
        <exposedName>queryWorkUnit</exposedName>
        <pythonFunction>testGridModule.queryWorkUnitTest2</pythonFunction>
    </queryWorkUnitMethod>
    <cancelWorkUnitMethod>
        <exposedName>cancelWorkUnit</exposedName>
        <pythonFunction>testGridModule.cancelWorkUnitTest2</pythonFunction>
    </cancelWorkUnitMethod>
    <callbackMethod>
        <exposedName>workUnitCallback</exposedName>
        <pythonFunction>scifloManagerTest.workUnitCallback</pythonFunction>
    </callbackMethod>
</scifloConfig>
