<?xml version="1.0"?>
<datasetInfos xmlns="http://sciflo.jpl.nasa.gov/2006v1/sf"
        xmlns:sf="http://sciflo.jpl.nasa.gov/2006v1/sf"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <dataset>
    <identification>  
      <ipath>eos::data/AIRS/RetStd/L2</ipath>
      <version>${version}</version>
      <fileTemplate>AIRS.${year}.${month}.${day}.L2.RetStd.${version}${stamp}.hdf</fileTemplate>
      <filePattern>(?P<source>AIRS)\.(?P<year>\d{4})\.(?P<month>\d{2})\.(?P<day>\d{2})\.(?P<level>L2)\.(?P<product>RetStd)\.(?P<version>v.+)\.(?P<stamp>.G[0-9]+)\.hdf$</filePattern>
      <metaFilePattern>${filePattern}.met$</metaFilePattern>
    </identification>
    <metadata>
      <sourceType>instrument</sourceType>
      <timeCoverage></timeCoverage>
      <spatialCoverage>swath</spatialCoverage>
      <timeResolution>30 seconds</timeResolution>
      <spatialResolution>10 km</spatialResolution>
      <fileCadence>6 minutes</cadence>
      <repeatCycle>16 days</repeatCycle>
      <extractor version='v3'>python:AIRSData.py?AIRSData.metadataExtract</extractor>
      <extractor version='v4'>python:AIRSData.py?AIRSData.metadataExtract</extractor>
    </metadata>
    <publishAt>
      <location>
        <data version='v3'>file://${hostname}/${SCIFLO_ROOT}/data/airs/L2/${year}/${month}/${day}</data>
      </location>
      <location>
        <data version='v4'>file://${hostname}/${SCIFLO_ROOT}/data/airsv4/L2/${year}/${month}/${day}</data>
      </location>
    </publishAt>
    <discoverAt>
      <location>
        <data version='v4'>ftp://g0dps01u.ecs.nasa.gov/AL22/AIRX2RET.002/</data>
      </location>
    </discoverAt>
  </dataset>
  <dataset>
    <identification>  
      <ipath>eos::IMPACT/AOD_Daily</ipath>
      <version>${version}</version>
      <fileTemplate>IMPACT.AOD_Daily.${year}.${month}.${day}.${version}.nc</fileTemplate>
      <filePattern>(?P<source>IMPACT)\.(?P<product>AOD_Daily)\.(?P<year>\d{4})\.(?P<month>\d{2})\.(?P<day>\d{2})\.(?P<version>v.+)\.nc$</filePattern>
      <metaFilePattern>None</metaFilePattern>
    </identification>
    <metadata>
      <sourceType>model</sourceType>
      <timeCoverage>@http://df3.jpl.nasa.gov/genesis/data/impact/daily/timeCoverage.xml</timeCoverage>
      <spatialCoverage>global</spatialCoverage>
      <timeResolution>1 day<timeResolution/>
      <spatialResolution>1 degree</spatialResolution>
      <fileCadence>1 day</cadence>
      <repeatCycle>None</repeatCycle>
      <extractor version='v1'>None</extractor>
    </metadata>
    <publishAt>
      <location>
        <data version='v1'>file://${hostname}/${SCIFLO_ROOT}/data/impact/daily/${year}</data>
      </location>
      <location>
        <data version='v2'>http://${hostname}/${SCIFLO_ROOT}/data/impact/daily/${year}</data>
      </location>
    </publishAt>
    <discoverAt>
      <location>
        <data version='v1'>http://amap.engin.umich.edu/sciflo/data/impact/daily/${year}</data>
      </location>
      <location>
        <data version='v1'>http://df3.jpl.nasa.gov/sciflo/data/impact/daily/${year}</data>
      </location>
    </discoverAt>
  </dataset>
</datasetInfos>
